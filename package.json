{
  "name": "fortellis",
  "version": "0.5.1",
  "description": "Fortellis APIs Client Library for Node.js",
  "main": "./build/src/index.js",
  "files": [
    "./build/**/*"
  ],
  "scripts": {
    "build": "tsc -p .",
    "lint": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "publish": "tsc -p . && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rrandall91/fortellis-api-nodejs-client.git"
  },
  "keywords": [
    "fortellis",
    "api",
    "elead",
    "cdk",
    "cdk drive"
  ],
  "author": "Rashaad R. Randall <hello@rashaadrandall.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/rrandall91/fortellis-api-nodejs-client/issues"
  },
  "homepage": "https://github.com/rrandall91/fortellis-api-nodejs-client#readme",
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "dotenv": "^16.0.3",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^29.4.2",
    "prettier": "2.2.1",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@types/uuid": "^9.0.1",
    "axios": "^1.3.1",
    "uuid": "^9.0.0"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageProvider": "v8",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**",
      "!**/tests/**"
    ],
    "verbose": true,
    "clearMocks": true,
    "passWithNoTests": true,
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    }
  }
}
